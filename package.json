        "astro": "^4.8.3",
        "framer-motion": "^11.2.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.2.0",
        "autoprefixer": "^10.4.19",
        "cssnano": "^7.0.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-astro": "^1.2.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "globals": "^15.2.0",
        "postcss": "^8.4.38",
